{
  "name": "talpa-etkinlik",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "test:all": "npm test && npm run e2e",
    "check:boundaries": "vitest run src/__tests__/architecture",
    "check:circular": "npx madge --circular --extensions ts,tsx src/modules",
    "arch:baseline": "npx tsx scripts/architecture/baseline.ts --save",
    "arch:baseline:check": "npx tsx scripts/architecture/baseline.ts",
    "arch:drift": "npx tsx scripts/architecture/drift-detector.ts",
    "arch:drift:fail": "npx tsx scripts/architecture/drift-detector.ts --fail-on-drift",
    "arch:metrics": "npx tsx scripts/architecture/metrics-reporter.ts",
    "arch:metrics:json": "npx tsx scripts/architecture/metrics-reporter.ts --format=json",
    "arch:metrics:md": "npx tsx scripts/architecture/metrics-reporter.ts --format=markdown --save",
    "arch:guard": "npx tsx scripts/architecture/ci-guard.ts",
    "arch:guard:strict": "npx tsx scripts/architecture/ci-guard.ts --strict",
    "arch:all": "npm run check:circular && npm run check:boundaries && npm run arch:guard"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.17.0",
    "jsdom": "^27.4.0",
    "madge": "^8.0.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.0"
  }
}
